{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "openhack1datafactory"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/CopyPipeline_kmv')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_kmv",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "."
							},
							{
								"name": "Destination",
								"value": "OrdersAggInfo"
							}
						],
						"typeProperties": {
							"source": {
								"type": "SqlMISource",
								"sqlReaderQuery": "select\n      t1.ItemId as id,\n       t2.CategoryName,\n        t1.ItemId,\n        t1.ProductName,\n        t1.Description,\n        t1.UnitPrice,\n        t1.Popularity,\n        t1.OriginalLanguage,\n        t1.ReleaseDate,\n        t1.VoteAverage\nfrom \n    Item t1, \n    Category t2 \nwhere \n    t1.CategoryId = t2.CategoryId",
								"partitionOption": "None"
							},
							"sink": {
								"type": "CosmosDbSqlApiSink",
								"writeBehavior": "upsert",
								"disableMetricsCollection": false
							},
							"enableStaging": false,
							"validateDataConsistency": false
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_kmv",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_kmv",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/copySqlServer2CosmosDB')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "ForEach_tz6",
						"type": "ForEach",
						"dependsOn": [],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@pipeline().parameters.cw_items",
								"type": "Expression"
							},
							"activities": [
								{
									"name": "Copy_tz6",
									"type": "Copy",
									"dependsOn": [],
									"policy": {
										"timeout": "7.00:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [
										{
											"name": "Source",
											"value": "dbo.@{item().source.table}"
										},
										{
											"name": "Destination",
											"value": "@{item().destination.collectionName}"
										}
									],
									"typeProperties": {
										"source": {
											"type": "SqlMISource",
											"partitionOption": "None"
										},
										"sink": {
											"type": "CosmosDbSqlApiSink",
											"writeBehavior": "insert"
										},
										"enableStaging": false,
										"validateDataConsistency": false
									},
									"inputs": [
										{
											"referenceName": "SourceDataset_tz6",
											"type": "DatasetReference",
											"parameters": {
												"cw_table": "@item().source.table"
											}
										}
									],
									"outputs": [
										{
											"referenceName": "DestinationDataset_tz6",
											"type": "DatasetReference",
											"parameters": {
												"cw_collectionName": "@item().destination.collectionName"
											}
										}
									]
								}
							]
						}
					}
				],
				"parameters": {
					"cw_items": {
						"type": "Array",
						"defaultValue": [
							{
								"source": {
									"table": "Cartitem"
								},
								"destination": {
									"collectionName": "CartItem"
								}
							},
							{
								"source": {
									"table": "Category"
								},
								"destination": {
									"collectionName": "Category"
								}
							},
							{
								"source": {
									"table": "Event"
								},
								"destination": {
									"collectionName": "Event"
								}
							},
							{
								"source": {
									"table": "Item"
								},
								"destination": {
									"collectionName": "Item"
								}
							},
							{
								"source": {
									"table": "ItemAggregate"
								},
								"destination": {
									"collectionName": "ItemAggregate"
								}
							},
							{
								"source": {
									"table": "OrderDetails"
								},
								"destination": {
									"collectionName": "OrderDetails"
								}
							},
							{
								"source": {
									"table": "Orders"
								},
								"destination": {
									"collectionName": "Orders"
								}
							},
							{
								"source": {
									"table": "User"
								},
								"destination": {
									"collectionName": "User"
								}
							}
						]
					}
				},
				"annotations": [],
				"lastPublishTime": "2020-11-12T09:19:08Z"
			},
			"dependsOn": []
		}
	]
}