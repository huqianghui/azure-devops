{
	"name": "copyBySQL",
	"properties": {
		"description": "copy data by sql for category name",
		"activities": [
			{
				"name": "Copy_c93",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "."
					},
					{
						"name": "Destination",
						"value": "OrdersAggInfo"
					}
				],
				"typeProperties": {
					"source": {
						"type": "SqlMISource",
						"sqlReaderQuery": "select\n      t1.ItemId as id,\n       t2.CategoryName,\n        t1.ItemId,\n        t1.ProductName,\n        t1.Description,\n        t1.UnitPrice,\n        t1.Popularity,\n        t1.OriginalLanguage,\n        t1.ReleaseDate,\n        t1.VoteAverage\nfrom \n    Item t1, \n    Category t2 \nwhere \n    t1.CategoryId = t2.CategoryId",
						"partitionOption": "None"
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "upsert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"validateDataConsistency": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"sink": {
									"path": "$"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_c93",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_c93",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}